		

			<div class="container">	
			<div class="row">		
			<h2>Search Results</h2>
			<hr>

				<div class="col-sm-8 content-background">
				
				<% @track_query.each do |x| %>
					<div class="audio-around box-shadow--6dp">
					<audio controls class="audio-widget" preload="metadata">
					  <source src="<%= x.track.url %>" type="audio/mpeg">
					Your browser does not support the audio element.
					</audio>

					<div class="song-content">
					<span style="font-size: 20px; padding-right: 10px;"> <%= x.title %></span>
					<em><%= x.artist %></em>
					
					</div><!-- song content -->
					</div><!-- audio-around -->
					<br><br>
					
				<% end %>

				<% @user_query.each do |x| %>
				<% if x.first_name %>
					<h2><%= link_to x.first_name, user_path(x.id) %></h2>
				<% else %>
					<h2><%= link_to x.username, user_path(x.id) %></h2>
				<% end %>
					
				<% end %>
				</div>


				

			</div>

				<div id="loading">
					
				</div>

			<h4 class="text-center"><%= will_paginate @track_query %></h4>

			</div>	