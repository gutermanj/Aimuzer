		<div class="tag-section">

				<% track.tags.each do |y| %>
					<div class="tag-div" data-id="<%= y.id %>" data-user="<%= current_user.id %>">
						<% if current_user.user_tags.find_by(tag_id: y.id) %>
							<button class="btn btn-default btn-xs tag-btns-disabled btn-space" type="submit" data-id="<%= y.id %>" data-user="<%= current_user.id %>"><%= y.characteristic %><%= y.id %></button>
						<% else %>
							<button class="btn btn-default btn-xs tag-btns btn-space" type="submit" data-id="<%= y.id %>" data-user="<%= current_user.id %>"><%= y.characteristic %><%= y.id %></button>
						<% end %>
					</div>
				<% end %>


				<% current_user.user_tags.each do |x| %>
					<%= x.tag_id %>
					<% end %>

		</div>